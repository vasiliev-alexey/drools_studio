<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="tableModels" maxHeight="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.av.ui.controllers.ModelTableViewController">
    <columns>
        <TableColumn fx:id="codeColumn" maxWidth="-1.0" minWidth="250.0" prefWidth="-1.0" text="Код" />
        <TableColumn fx:id="nameColumn" editable="false" maxWidth="-1.0" minWidth="500.0" prefWidth="-1.0" text="Наименование" />
    </columns>
    <placeholder>
        <Label text="Нет данных" />
    </placeholder>
</TableView>
