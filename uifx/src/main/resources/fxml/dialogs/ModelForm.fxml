<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="401.0" prefWidth="713.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.av.ui.controllers.dilalogs.ModelFormController">
    <children>
        <GridPane alignment="BASELINE_CENTER" hgap="5.0" prefHeight="96.0" prefWidth="680.0" vgap="3.0"
                  AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="167.0" minWidth="10.0"
                                   prefWidth="95.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="33.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="34.0" minHeight="10.0" prefHeight="27.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Код модели"/>
                <Label text="Наименование" GridPane.rowIndex="1"/>
                <Label text="Пакет" GridPane.rowIndex="2"/>
                <TextField fx:id="txtCode" prefHeight="25.0" prefWidth="110.0" GridPane.columnIndex="1"/>
                <TextField fx:id="txtName" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="txtPackage" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </GridPane>
        <TableView fx:id="tableGroup" editable="true" layoutY="115.0" prefHeight="222.0" prefWidth="680.0"
                   AnchorPane.bottomAnchor="79.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
                   AnchorPane.topAnchor="100.0">
            <columns>
                <TableColumn fx:id="codeColumn" maxWidth="285.0" minWidth="50.0" prefWidth="231.0" text="Код группы" >

                </TableColumn>
                <TableColumn fx:id="nameColumn" maxWidth="454.0" minWidth="45.0" prefWidth="337.0"
                             text="Наименование группы"  />
                <TableColumn fx:id="typeColumn" maxWidth="1.7976931348623157E308" minWidth="6.0" prefWidth="138.0"
                             text="Тип"/>
            </columns>
        </TableView>

        <HBox id="HBox" alignment="CENTER"   spacing="5.0" AnchorPane.bottomAnchor="10.0"
              AnchorPane.rightAnchor="10.0">
            <children>
                <Button mnemonicParsing="false" onAction="#handleOk" prefWidth="80.0" text="OK"/>
                <Button mnemonicParsing="false" onAction="#handleCancel" prefWidth="80.0" text="Cancel"/>
            </children>
        </HBox>


    </children>
</AnchorPane>
